{
  "name": "sneakerproject",
  "version": "1.0.0",
  "description": "Sneaker tracking application",
  "main": "server.js",
  "scripts": {
    "begin": "npm run build && node server.js",
    "start": "node server.js",
    "start:original": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wron1/SneakerProject.git"
  },
  "author": "Daniel Moore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wron1/SneakerProject/issues"
  },
  "homepage": "https://github.com/wron1/SneakerProject#readme",
  "dependencies": {
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.14.0",
    "sneaks-api": "^1.2.3"
  }
}
